#include <iostream>

using namespace std;


int main() {

    int size;
    cout<<"Enter size of random array: ";
    cin>>size;
    int arr[size];
    arr[0]=rand()%1000;
    cout<<arr[0]<<" ";
    for(int i=1;i<size;i++){
        arr[i]=rand()%1000;
        cout<<arr[i]<<" ";
    }

    return 0;
}
